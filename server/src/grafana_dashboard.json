{
  "dashboard": {
    "title": "Oxidize Dashboard",
    "tags": [
      "relay",
      "networking"
    ],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "oxidize_relay_connections_active",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Total Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "oxidize_relay_connections_total",
            "legendFormat": "Total"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Throughput (Bytes/sec)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(oxidize_relay_bytes_sent_total[1m])",
            "legendFormat": "Sent"
          },
          {
            "expr": "rate(oxidize_relay_bytes_received_total[1m])",
            "legendFormat": "Received"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Packet Rate (Packets/sec)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(oxidize_relay_packets_sent_total[1m])",
            "legendFormat": "Sent"
          },
          {
            "expr": "rate(oxidize_relay_packets_received_total[1m])",
            "legendFormat": "Received"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Compression Savings",
        "type": "stat",
        "targets": [
          {
            "expr": "oxidize_relay_compression_saved_bytes",
            "legendFormat": "Bytes Saved"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Request Duration (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(oxidize_relay_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{operation}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "version": 1
  }
}