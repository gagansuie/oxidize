{
    "$schema": "https://schema.tauri.app/config/2/capability",
    "identifier": "default",
    "description": "Default capabilities for Oxidize VPN",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "shell:allow-open",
        "notification:default",
        "autostart:default",
        "store:default",
        "updater:default",
        "process:allow-restart",
        "process:allow-exit"
    ]
}